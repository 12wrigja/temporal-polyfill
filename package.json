{
  "name": "temporal",
  "version": "1.0.0",
  "description": "TC39 Temporal Proposal Polyfill",
  "main": "index.js",
  "module": "lib/index.mjs",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "scripts": {
    "test": "for FILE in $(find test -type f -name '*.js' | grep -v 'test/node.js'); do node --harmony-bigint -r ./test/node.js $FILE; done",
